{
  "GeneralConfiguration": {
    "StartupWaitingTimeInMinutes" : "60"
  },
  "AzureConfiguration": {
    "AzureBlobConnectionString": "#{AzureBlobConnectionString}#"
  },
  "VericoinConfiguration": {
    "Enabled": "true",
    "AzureBlobContainerName": "pivericoin",
    "CryptoLocalDirectoryFullName": "#{VericoinLocalDirectoryFullName}#",
    "DockerContainerName": "vericoin"
  },
  "VeriumConfiguration": {
    "Enabled": "true",
    "AzureBlobContainerName": "piverium",
    "CryptoLocalDirectoryFullName": "#{VeriumLocalDirectoryFullName}#",
    "DockerContainerName": "verium"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
